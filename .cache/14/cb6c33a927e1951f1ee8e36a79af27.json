me":"player","generated":{"line":65,"column":0},"source":"bounce.js","original":{"line":63,"column":0}},{"generated":{"line":65,"column":6},"source":"bounce.js","original":{"line":63,"column":6}},{"name":"init","generated":{"line":65,"column":7},"source":"bounce.js","original":{"line":63,"column":7}},{"generated":{"line":65,"column":11},"source":"bounce.js","original":{"line":63,"column":0}}],"sources":{"bounce.js":"\r\nconst player = {\r\n    id: undefined,\r\n    n: 1,\r\n    time: 100,\r\n    events: {\r\n        '#btnPlay': 'play',  //定义的时候不能使用player.play 等 一开始的player是一个undefined\r\n        '#btnPause': 'pause',\r\n        '#btnSlow': 'slow',\r\n        '#btnNormal': 'normal',\r\n        '#btnFast': 'fast'\r\n    },\r\n    ui:{\r\n        demo : document.querySelector(\"#demo\"),\r\n        demo2 : document.querySelector(\"#demo2\")\r\n    },\r\n    init: () => {\r\n        player.ui.demo.innerText = string.substr(0, player.n)\r\n        player.ui.demo2.innerHTML = string.substr(0, player.n)\r\n        player.play()\r\n        player.bindEvents()\r\n    },\r\n    bindEvents: () => {\r\n        for (let key in player.events) if (player.events.hasOwnProperty(key)) {\r\n            const value = player.events[key] //用键驱除值\r\n            document.querySelector(key).onclick = player[value] //player[value]相当于player.value\r\n        }\r\n    },\r\n    run: () => {\r\n        if (player.n > string.length) {\r\n            window.clearInterval(player.id)\r\n            return\r\n        }\r\n        player.n += 1\r\n        player.ui.demo.innerText = string.substr(0, player.n)\r\n        player.ui.demo2.innerHTML = string.substr(0, player.n)\r\n        player.ui.demo.scrollTop = player.ui.demo.scrollHeight\r\n    },\r\n    play: () => {\r\n        player.id = setInterval(player.run, player.time)\r\n        //setInterval(()=>{run()},time)和setInterval(run,time)等价 除非run返回一个函数不然不能加括号\r\n    },\r\n    pause: () => {\r\n        window.clearInterval(player.id)\r\n    },\r\n    slow: () => {\r\n        player.pause()\r\n        player.time = 300\r\n        player.play()\r\n    },\r\n    normal: () => {\r\n        player.pause()\r\n        player.time = 100\r\n        player.play()\r\n    },\r\n    fast: () => {\r\n        player.pause()\r\n        player.time = 0\r\n        player.play()\r\n    }\r\n}\r\n\r\nplayer.init()\r\n\r\n"},"lineCount":null}},"error":null,"hash":"cb98cf19e2ad89e45319f021fc06e00b","cacheData":{"env":{}}}